                   internal_properties : 
                   type : 4
                   code_elements :
                    type_code : 4
                    p_codes :
                     -
                       code : |1-
                        //On re-sauve la commande avant, au cas où il y aurait eu des modifications .. 
                        ExécuteTraitement(BTN_Sauver,trtClic)
                        
                        //Création de la facture
                        sNumFacture est une chaine = factureCrée()
                        si sNumFacture = "" ALORS
                        	erreur("Erreur ! Facturation impossible ! ")
                        	retour
                        FIN
                        
                        facture.UUIDClient = commande.UUIDClient
                        
                        facture.NumCommande = commande.NumCommande
                        facture.DateCommande = commande.DateCommande
                        
                        facture.TotalHT = commande.TotalHT
                        facture.Totaltva = SAI_TotalTVA
                        facture.TotalTTC = commande.TotalTTC
                        facture.TotalFraisPort = commande.FraisPort
                        
                        //Adresse de facturation en clair, et normalisée
                        facture.Adressefacturation = SAI_AdresseFactNomComplet + [RC] +  SAI_AdresseFactComplément +  [RC] + sAI_AdresseFactVoie + [rc] + SAI_AdresseFactBoîtePostale + [RC] + SAI_AdresseCodePostal +  [esp] +  majuscule(SAI_AdresseFactVille) + [rc] + majuscule(cOMBO_AdresseFactPays[COMBO_AdresseFactPays])
                        
                        //Ajout de la facture en base et ajout du numéro de facture à la Commande correspondante
                        si hmodifie(facture) alors
                        	
                        	commande.NumFacture = sNumFacture 
                        	HModifie(commande)
                        	
                        fin
                        
                        Notifie("Ajout.Document", Facture.IDFacture)	
                        
                        //Fermeture de l'onglet
                        OngletFerme()
