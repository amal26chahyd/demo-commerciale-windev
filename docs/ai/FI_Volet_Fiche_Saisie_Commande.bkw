  procedures :
   -
     name : CalculeTotaux
     procedure_id : 1628274440395126609
     type_code : 14
     code : |1-
      // Résumé : Calcule les totaux 
      // Syntaxe :
      //CalculeTotaux ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE CalculeTotaux()
      
      
      rTotalTVA est un réel
      rTotalHT est un réel
      rTotalTTC est un réel
      
      
      // Parcours des lignes de la table
      POUR TOUTE LIGNE DE TABLE_LigneCde
      	
      	// Mémorise le total HT
      	rTotalHT += TABLE_LigneCde.COL_SousTotalht
      	// Mémorise le total TVA
      	rTotalTVA +=(TABLE_LigneCde.COL_SousTotalHT*(TABLE_LigneCde.COL_TauxTVA))
      		
      	
      FIN
      
      // Calcule le total TTC
      rTotalTTC = rTotalHT + rTotalTVA 
      
      // Saisie des totaux dans les champs
      SAI_TotalHT = rTotalHT
      SAI_TotalTVA = rTotalTVA
      SAI_TotalTTC = rTotalTTC
      
      
      LIB_Total = SAI_FraisPort + SAI_TotalTTC
     type : 458752
   -
     name : SaisieAssistée
     internal_properties : BgAAAAYAAAAFwB6dZyp6ynvj4YZcAUz74XRxZC7aVZSP1xAPar/XdVFgux4W3tQUPiRZi/n5dhKxh+xtPzVie0HI9H5gxX1GwWmaiYmy+cBVADXhNZui9YLDyI2gpmvkMwqSTkEbWSCipe9S0pZQ/9D8+jit474lKMMKqkH525GBVHEQZUK1SfVl9PM=
     procedure_id : 2024323970442111286
     type_code : 14
     code : |1-
      // Résumé : Saisie assistée 
      // Syntaxe :
      //SaisieAssistée ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code, avec un différé de 30 centièmes de seconde
      // 
      
      PROCEDURE SaisieAssistée()
      
      // Si les saisies sont vides, ne rien faire
      si SAI_Ajout_Référence ~= "" _et_ SAI_Ajout_Libellé ~= "" alors
      	// Ferme la saisie assistée
      	MaFenêtreInterne..Plan = 0
      	retour
      FIN
      
      // Recherche les 10 premiers produits correspondants à la saisie
      REQ_SaisieAssistéeAjoutProduit.ParamReference = (SAI_Ajout_Référence ~= "" ? null SINON SAI_Ajout_Référence)
      REQ_SaisieAssistéeAjoutProduit.ParamLibelleDeclinaison = (SAI_Ajout_Libellé ~= "" ? Null SINON SAI_Ajout_Libellé)
      si HExécuteRequête(REQ_SaisieAssistéeAjoutProduit) = faux ALORS
      	// Ferme la saisie assistée
      	MaFenêtreInterne..Plan = 0
      	Erreur("Impossible d'exécuter la requête de saisie assistée")
      	retour
      FIN
      
      // Selon le nombre de retours
      selon HNbEnr(REQ_SaisieAssistéeAjoutProduit)
      	// Si aucune référence
      	CAS 0
      		// Ne rien faire
      		toastaffiche("Aucun produit ne correspond à la saisie", toastCourt, cvmilieu, chcentre)
      		reprisesaisie(SAI_Ajout_Référence)
      		
      	// 1 seul résultat
      	CAS 1
      		// Lit l'unique résultat
      		hlitpremier(REQ_SaisieAssistéeAjoutProduit)
      		// Mémorise les informations dans les champs de saisie
